<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/primerFragment">

    <fragment
        android:id="@+id/primerFragment"
        android:name="com.precafe.precafe.PrimerFragment"
        android:label="fragment_primer"
        tools:layout="@layout/fragment_primer" >
        <action
            android:id="@+id/action_primerFragment_to_begudesFragment"
            app:destination="@id/begudesFragment" />
    </fragment>
    <fragment
        android:id="@+id/begudesFragment"
        android:name="com.precafe.precafe.BegudesFragment"
        android:label="fragment_begudes"
        tools:layout="@layout/fragment_begudes" >
        <action
            android:id="@+id/action_begudesFragment_to_totalFragment"
            app:destination="@id/totalFragment" />
    </fragment>
    <fragment
        android:id="@+id/totalFragment"
        android:name="com.precafe.precafe.TotalFragment"
        android:label="fragment_total"
        tools:layout="@layout/fragment_total" />
</navigation>